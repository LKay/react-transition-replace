{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "es6", "scripthost"],
    "outDir": "lib",
    "module": "commonjs",
    "moduleResolution": "node",
    "declaration": true,
    "sourceMap": true,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "strictNullChecks": false,
    "jsx": "react"
  },
  "include": [
    "./"
  ],
  "exclude": [
    "node_modules"
  ]
}
