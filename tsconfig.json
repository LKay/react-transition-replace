{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "target": "es5",
    "downlevelIteration": true,
    "importHelpers": false,
    "sourceMap": true,
    "declaration": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "jsx": "react",
    "allowJs": false,
    "checkJs": false,
    "lib": [
      "dom",
      "es2015",
      "es2016",
      "es2017",
      "esnext"
    ],
    "forceConsistentCasingInFileNames": true,
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "noUnusedParameters": false,
    "noImplicitReturns": false,
    "noImplicitThis": false,
    "noUnusedLocals": false,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "suppressImplicitAnyIndexErrors": true,
    "baseUrl": ".",
    "paths": {
      "*": ["types/*"]
    }
  },
  "files": [
    "src/index.ts",
    "src/TransitionReplace.tsx",
    "src/react-router/SwitchTransition.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
